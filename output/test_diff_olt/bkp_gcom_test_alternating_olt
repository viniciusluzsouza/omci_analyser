![IF]
interface vlan-interface 10 
ip address 10.10.10.200 255.255.255.0 
exit 
interface meth-interface 0 
ip address 192.168.10.1 255.255.255.0 
exit 
![deploy-profile-vlan]
deploy profile vlan
aim 1
translate old-vlan 1234 new-vlan 1234
active
aim 2
translate old-vlan 123 new-vlan 123
translate old-vlan 321 new-vlan 321
active
aim 3
translate old-vlan 11 new-vlan 11
translate old-vlan 22 new-vlan 22
translate old-vlan 33 new-vlan 33
translate old-vlan 44 new-vlan 44
active
aim 4
translate old-vlan 2222 new-vlan 2222
active
exit
![deploy-profile-dba]
deploy profile dba
aim 1
type 4 max 1200000
active
exit
![deploy-profile-line]
deploy profile line
aim 1
device type i41-100
tcont 1 profile dba 1
gemport 1 tcont 1 vlan-profile 1
mapping mode port-vlan
mapping 1 port eth 1 vlan 1234 gemport 1
flow 1 port eth 1 default vlan 1234
active
aim 2
device type i41-100
tcont 1 profile dba 1
tcont 2 profile dba 1
gemport 1 tcont 1 vlan-profile 2
gemport 2 tcont 2 vlan-profile 2
mapping mode port-vlan
mapping 1 port eth 1 vlan 123 gemport 1
mapping 2 port eth 1 vlan 321 gemport 2
flow 1 port eth 1 default vlan 123
flow 2 port eth 1 vlan 321 translate vlan 100
active
aim 3
device type i41-421
tcont 1 profile dba 1
tcont 2 profile dba 1
tcont 3 profile dba 1
tcont 4 profile dba 1
gemport 1 tcont 1 vlan-profile 3
gemport 2 tcont 2 vlan-profile 3
gemport 3 tcont 3 vlan-profile 3
gemport 4 tcont 4 vlan-profile 3
mapping mode port-vlan
mapping 1 port eth 1 vlan 11 gemport 1
mapping 2 port eth 2 vlan 22 gemport 2
mapping 3 port eth 3 vlan 33 gemport 3
mapping 4 port eth 4 vlan 44 gemport 4
flow 1 port eth 1 default vlan 11
flow 2 port eth 2 default vlan 22
flow 3 port eth 3 default vlan 33
flow 4 port eth 4 default vlan 44
active
aim 4
device type i41-421
tcont 1 profile dba 1
gemport 1 tcont 1 vlan-profile 4
mapping mode port-vlan
mapping 1 port veip vlan 2222 gemport 1
flow 1 port veip vlan 222 keep
active
exit
![deploy-profile-rule]
deploy profile rule
aim 0/1/1
permit sn string-hex ABCD-0d5f19a4 line 2 default line 2
active
aim 0/1/2
permit sn string-hex ZNTS-037268bd line 4 default line 4
active
exit
